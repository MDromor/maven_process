pipeline{
	agent any
	stages{
		stage('HELLO'){
			steps{
			echo "hello world"
			}

		}
		stage('BUILD'){
		    steps{
		        echo "Putting it all together"
		    }
		}
		stage ('STAGING'){
		    steps{
		        echo "Final Testing"
		    }
		}
		stage ('PRODUCTION'){
			steps{
			echo "We are live"
			}
		}


	}

}